{
  "name": "@prowire-vpn/server",
  "version": "1.0.0",
  "description": "Prowire serve",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "swc src -d dist --ignore **/*.test.ts && npm run types:build && tsc-alias -p tsconfig.build.json",
    "types:build": "tsc -p tsconfig.build.json --composite false --emitDeclarationOnly",
    "types:check": "tsc -p tsconfig.test.json --noEmit",
    "lint": "eslint .",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "watch": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prowire-vpn/prowire.git"
  },
  "author": "Noe Charmet",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/prowire-vpn/prowire/issues"
  },
  "homepage": "https://github.com/prowire-vpn/prowire#readme",
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/event-emitter": "^1.4.2",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/platform-ws": "^9.4.3",
    "@nestjs/websockets": "^9.4.3",
    "bson": "^4.7.2",
    "express": "^4.18.2",
    "joi": "^17.10.2",
    "rxjs": "^7.8.1",
    "telnet-client": "^2.0.8",
    "tmp": "^0.2.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.4.3",
    "@prowire-vpn/api": "*",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.88",
    "@swc/jest": "^0.2.29",
    "@types/chai": "^4.3.6",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.17.19",
    "@types/tmp": "^0.2.4",
    "@types/ws": "^8.5.5",
    "chai": "^4.3.8",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
