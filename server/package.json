{
  "name": "@prowire-vpn/server",
  "version": "1.0.0",
  "description": "Prowire serve",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"./src/**/*.ts\" --ext .ts",
    "build": "rm -rf  dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:run": "sudo node -r dotenv/config ./dist/main.js",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "watch": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prowire-vpn/prowire.git"
  },
  "author": "Noe Charmet",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/prowire-vpn/prowire/issues"
  },
  "homepage": "https://github.com/prowire-vpn/prowire#readme",
  "dependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/platform-ws": "^9.2.0",
    "@nestjs/websockets": "^9.2.0",
    "bson": "^4.7.0",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "rxjs": "^7.5.7",
    "telnet-client": "^2.0.6",
    "tmp": "^0.2.1",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@prowire-vpn/api": "^1.0.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/tmp": "^0.2.3",
    "@types/ws": "^8.5.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  }
}
